

<div class="panel panel-default">
                    <!-- <div class="panel-heading">
                        <h3 class="panel-title text-center">Please Register</h3>
                    </div> -->
                    <div class="panel-body">
<h1 style="text-align: center;">Current Users</h1>

<table class="table table-stripped">
  <thead class="table table-stripped">
    <tr>
      <th>Username</th>
      <th>Hostname</th>
      <th>Emailaddress</th>
      <th>Timeserver</th>
      <th>Password</th>
      <th>Location</th>
      <th>Flashtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="table table-stripped">
    <% @users.each do |user| %>
      <tr>
        <td><%= user.userName %></td>
        <td><%= user.hostName %></td>
        <td><%= user.emailAddress %></td>
        <td><%= user.timeServer %></td>
        <td><%= user.password %></td>
        <td><%= user.location %></td>
        <td><%= user.flashTime %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
  <%= link_to " Logout", :controller=> "sessions", :action=> "logout" %>
                  
</div>

        </div>
